<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>SAdmin</servlet-name>
        <servlet-class>com.servlet.SAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SUsuario</servlet-name>
        <servlet-class>com.servlet.SUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SAdmin</servlet-name>
        <url-pattern>/SAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SUsuario</servlet-name>
        <url-pattern>/SUsuario</url-pattern>
    </servlet-mapping>
    <filter>
	<filter-name>CORS</filter-name>
	<filter-class>com.thetransactioncompany.cors.CORSFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CORS</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
